{
  "name": "integration_tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./target",
    "bundle": "npm run _clean:dist && npm run _build:rust:prod && cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cpy ./site/static ./dist/static && cpy ./site/_redirects ./dist/_redirects",
    "bundle:no-opt": "npm run _clean:dist && npm run _build:rust:prod-no-opt && cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cpy ./site/static ./dist/static && cpy ./site/_redirects ./dist/_redirects",
    "dev": "npm run _build:rust:dev && npm-run-all -p _dev:server _dev:webpack-server _dev:watch:rust _dev:localcdn",
    "_dev:server": "http-server ../assets",
    "_dev:localcdn": "node ./dev-utils/localcdn.js",
    "_clean:dist": "rimraf ./dist",
    "_clean:rust": "rimraf ./target",
    "_dev:webpack-server": "cross-env NODE_ENV=development webpack-dev-server --progress --open --config webpack.dev.js",
    "_dev:watch:rust": "chokidar \"../../**/*.rs\" \"../../**/*.glsl\" -c \"npm run _build:rust:dev\"",
    "_build:rust:prod": "cargo +nightly build --target wasm32-unknown-unknown --release && npm run _rust:bindgen:prod",
    "_build:rust:prod-no-opt": "cargo +nightly build --target wasm32-unknown-unknown --release && npm run _rust:bindgen:prod-no-opt",
    "_build:rust:dev": "cargo +nightly build --target wasm32-unknown-unknown && npm run _rust:bindgen:dev",
    "_rust:bindgen:dev": "make-dir ./target && cpy ../../target/wasm32-unknown-unknown/debug/integration_tests* ./target/ && wasm-bindgen ./target/integration_tests.wasm --out-dir ./target",
    "_rust:bindgen:prod": "make-dir ./target && cpy ../../target/wasm32-unknown-unknown/release/integration_tests* ./target/ && wasm-bindgen ./target/integration_tests.wasm --out-dir ./target && wasm-opt -Os -o ./target/integration_tests_bg.wasm ./target/integration_tests_bg.wasm",
    "_rust:bindgen:prod-no-opt": "make-dir ./target && cpy ../../target/wasm32-unknown-unknown/release/integration_tests* ./target/ && wasm-bindgen ./target/integration_tests.wasm --out-dir ./target"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "chokidar-cli": "^1.2.1",
    "cors": "^2.8.5",
    "cpy-cli": "^2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "html-webpack-plugin": "3.2.0",
    "http-server": "^0.11.1",
    "make-dir-cli": "^2.0.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "sass-loader": "^7.1.0",
    "serve-index": "^1.9.1",
    "source-map-loader": "0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "ts-loader": "5.3.3",
    "ts-shader-loader": "^1.0.5",
    "typescript": "3.3.3",
    "webpack": "4.29.4",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  }
}
