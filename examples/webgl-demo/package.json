{
  "name": "webgl-demo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist/* && rimraf ./target",
    "bundle": "npm run _clean:dist && npm run _build:rust:prod && cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cp -R ./site/static ./dist/ && cp ./site/_redirects ./dist/",
    "dev": "npm run _build:rust:dev && npm-run-all -p _dev:webpack-server _dev:watch:rust",
    "_clean:dist": "rimraf ./dist",
    "_clean:rust": "rimraf ./target",
    "_dev:webpack-server": "cross-env NODE_ENV=development webpack-dev-server --progress --open --config webpack.dev.js",
    "_dev:watch:rust": "chokidar \"../../**/*.rs\" \"../../**/*.glsl\" -c \"npm run _build:rust:dev\"",
    "_build:rust:prod": "cargo +nightly build --target wasm32-unknown-unknown --release && npm run _rust:bindgen:prod",
    "_build:rust:dev": "cargo +nightly build --target wasm32-unknown-unknown && npm run _rust:bindgen:dev",
    "_rust:bindgen:dev": "mkdir -p ./target && cp ../../target/wasm32-unknown-unknown/debug/webgl*demo* ./target/ && wasm-bindgen ./target/webgl_demo.wasm --out-dir ./target",
    "_rust:bindgen:prod": "mkdir -p ./target && cp ../../target/wasm32-unknown-unknown/release/webgl*demo* ./target/ && wasm-bindgen ./target/webgl_demo.wasm --out-dir ./target"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "chokidar-cli": "^1.2.1",
    "cross-env": "5.2.0",
    "css-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "0.5.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "1.1.0",
    "ts-loader": "5.3.1",
    "ts-shader-loader": "^1.0.5",
    "typescript": "3.2.1",
    "webpack": "4.27.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
